package produtos;

import basicas.Eletronico;

public class Smartphone extends Eletronico{
	
	public Smartphone(int id, String tipo, String fabricante, double preco) {
		super(id, tipo, fabricante, preco);
	}

	@Override
	public void ligar() {
		System.out.println("liga");
		
	}

	@Override
	public void desligar() {
		System.out.println("desliga");
		
	}

	@Override
	public void aumentaSom() {
		System.out.println("Aumenta som");
	}

	public void fazerChamada() {
		System.out.println("Alo");
	}
}
